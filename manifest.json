{
    "manifest_version": 3,
    "name": "Better File Names - GIU",
    "version": "1.0.0",
    "description": "Rename downloaded files with more readable names.",
    "permissions": ["downloads"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "foldericon.png"
    },
    "content_scripts": [
    {
      "matches": ["https://cms.giu-uni.de/*"],
      "js": ["./content.js"]
    }
    ]
  }
  